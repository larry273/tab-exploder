{
  "name": "Tab-Exploder",
  "version": "1.0",
  "description": "Explodes tabs into an overview selection",
  "permissions": ["tabs","activeTab","<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Do something"
	},
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2,
  "commands": {
    "show-exploder": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space",
        "linux": "Ctrl+Shift+Space",
        "windows": "Ctrl+Shift+Space",
        "mac": "Alt+Shift+Space"
      },
      "description": "Open the tab exploder"
    }
  }
}
