{
  "name": "Tab-Exploder",
  "version": "1.0",
  "description": "Explodes tabs into an overview selection",
  "permissions": ["tabs","activeTab","<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Show Tab-Exploder"
	},
  "icons": {
    "128": "images/icon.png"
  },
  "manifest_version": 2,
  "commands": {
    "show-exploder": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space",
        "linux": "Ctrl+Shift+Space",
        "windows": "Ctrl+Shift+Space",
        "mac": "Alt+Shift+Space"
      },
      "description": "Open Tab-Exploder"
    }
  }
}
